    "evaluators":
    {
        "isAlfrescoV42":
        {
            "match-any": ["isAlfrescoV42-EE", "isAlfrescoV42-CE"]
        },
        "isAlfrescoV42-EE":
        {
            "type": "com.alfresco.client.evaluator.isRepositoryVersion",
            "params":
            {
                "operator": ">=",
                "edition": "Enterprise",
                "majorVersion": 4,
                "minorVersion": 2
            }
        },
        "isAlfrescoV42-CE":
        {
            "type": "com.alfresco.client.evaluator.isRepositoryVersion",
            "params":
            {
                "operator": ">=",
                "edition": "Community",
                "majorVersion": 4,
                "minorVersion": 2
            }
        },
